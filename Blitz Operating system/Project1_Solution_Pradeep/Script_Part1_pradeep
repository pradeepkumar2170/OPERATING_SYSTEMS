[?2004lcode Hello

  -- This is the "hello world" program, for use in Project 1.

  function main ()
      print ("Hello, world...\n")
      --foo (10)
    endFunction

  function foo (x: int)
      bar (x+1)
    endFunction

  function bar (a: int)
    var b: int
      b = a + 1
      print ("The value of b is ")
      printInt (b)
      nl ()
      debug
      bar (b)
    endFunction

endCode
[?2004h]0;pradeep@pradeep-VirtualBox: ~/Proj1Files[01;32mpradeep@pradeep-VirtualBox[00m:[01;34m~/Proj1Files[00m$ cat HelloWorld.k
[?2004lcode Hello

  -- This is the "hello world" program, for use in Project 1.

  function main ()
      print ("Hello, world...\n")
      foo (10)
    endFunction

  function foo (x: int)
      bar (x+1)
    endFunction

  function bar (a: int)
    var b: int
      b = a + 1
      print ("The value of b is ")
      printInt (b)
      nl ()
      debug
      bar (b)
    endFunction

endCode
